        const TEACHING_IMPORT_ALIASES = {
            title: ['title', 'ten mon', 'ten hoc phan', 'mon', 'hoc phan', 'subject', 'chu de'],
            date: ['date', 'ngay', 'ngay day', 'ngay hoc'],
            start_time: ['gio bat dau', 'start time', 'start', 'gio vao', 'gio bd', 'tiet bat dau'],
