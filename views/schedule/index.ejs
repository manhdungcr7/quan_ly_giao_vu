                console.error('L·ªói khi x·ª≠ l√Ω thao t√°c gi·∫£ng vi√™n t√πy ch·ªânh:', error);
                alert(error.message || 'Kh√¥ng th·ªÉ th·ª±c hi·ªán thao t√°c. Vui l√≤ng th·ª≠ l·∫°i.');
