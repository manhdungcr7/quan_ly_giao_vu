                    }
                    teachingState.cache.clear();
                    loadTeachingSchedule(true);
                    alert(`${error.message || 'Kh√¥ng th·ªÉ t·∫°o l·ªãch gi·∫£ng'}\n\nL∆∞u √Ω: ${successfulSessions.length} khung gi·ªù ƒë·∫ßu ti√™n ƒë√£ ƒë∆∞·ª£c l∆∞u th√†nh c√¥ng.`);
                } else {
                    alert(error.message || 'Kh√¥ng th·ªÉ t·∫°o l·ªãch gi·∫£ng');
                }
                setTeachingLessonLoading(false);
            }
        }

