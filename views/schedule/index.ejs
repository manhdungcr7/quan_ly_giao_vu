        async function commitTeachingImport() {
            if (!teachingImportState.jobId) {
                alert('Vui l√≤ng t·∫£i file Excel v√† gh√©p c·ªôt tr∆∞·ªõc khi import.');
                return;
            }

            const missing = [];
            const payloadMapping = {};

            TEACHING_IMPORT_FIELDS.forEach(field => {
                const value = teachingImportState.mapping[field.key];
                if (field.required) {
                    if (!value) {
                        missing.push(field.label);
                    } else {
                        payloadMapping[field.key] = value;
                    }
                } else if (value) {
                    payloadMapping[field.key] = value;
                }
            });

            if (missing.length) {
                alert(`Vui l√≤ng gh√©p c·ªôt cho c√°c tr∆∞·ªùng b·∫Øt bu·ªôc: ${missing.join(', ')}`);
                return;
            }

            setTeachingImportStatus('ƒêang nh·∫≠p d·ªØ li·ªáu v√†o h·ªá th·ªëng...', 'info');
            setCommitButtonDisabled(true);

            try {
                const allowConflicts = document.getElementById('importAllowConflicts')?.checked;
                const response = await fetch('/api/schedule/teaching/import/commit', {
                    method: 'POST',
                    headers: { 'Content-Type': 'application/json' },
                    body: JSON.stringify({
                        jobId: teachingImportState.jobId,
                        mapping: payloadMapping,
                        options: { allowConflicts }
                    })
                });

                const result = await response.json();
                if (!response.ok) {
                    throw new Error(result.error || 'Kh√¥ng th·ªÉ nh·∫≠p d·ªØ li·ªáu');
                }

                renderTeachingImportErrors(result.errors || []);
                const statusMessage = result.failed
                    ? `ƒê√£ nh·∫≠p ${result.success}/${result.total} d√≤ng. ${result.failed} d√≤ng g·∫∑p l·ªói, vui l√≤ng ki·ªÉm tra danh s√°ch b√™n d∆∞·ªõi.`
                    : `ƒê√£ nh·∫≠p th√†nh c√¥ng ${result.success}/${result.total} d√≤ng d·ªØ li·ªáu.`;
                setTeachingImportStatus(statusMessage, result.failed ? 'warning' : 'success');

                teachingImportState.jobId = null;
                if (!result.failed) {
                    teachingImportState.mapping = {};
                }
                updateCommitButtonState();

                calendar.refetchEvents();
                teachingState.cache.clear();
                loadTeachingSchedule(true);

                if (!result.failed) {
                    alert('Nh·∫≠p l·ªãch gi·∫£ng th√†nh c√¥ng!');
                    toggleTeachingImportPanel(false);
                } else {
                    alert(`ƒê√£ nh·∫≠p ${result.success} d√≤ng, c√≤n ${result.failed} d√≤ng g·∫∑p l·ªói. Vui l√≤ng ki·ªÉm tra chi ti·∫øt.`);
                    setElementActive('teachingImportActions', true);
                }
            } catch (error) {
                console.error('Teaching import commit error:', error);
                setTeachingImportStatus(error.message || 'Kh√¥ng th·ªÉ nh·∫≠p d·ªØ li·ªáu', 'error');
                updateCommitButtonState();
            }
        }

